extend layout
block content
  div(class="container") 
    h1(style="margin-top:250px;") Online Users: 
      span#online-player-count
    ul#messages
    form(id="chat")
      input(id="chat-value" autocomplete="off" autofocus)
      input(type="submit" value="Send")
  div.box#master-suit-box
    button.suit
      img(src="/images/SmallC.png")
    button.suit
      img(src="/images/SmallD.png")
    button.suit
      img(src="/images/SmallH.png")
    button.suit
      img(src="/images/SmallS.png")
    button.suit.active
      div.overlay
      img(src="/images/SmallA.png")
    div#master-score-wrapper
      input#master-score(type="number" min="13" max="20" value="13" maxlength="2")
    div#master-run-wrapper
      button.btn.btn-primary 출마
      button.btn.btn-default 포기 
  div#hand0.hand
  div#hand1.hand
  div#hand2.hand
  div#hand3.hand
  div#hand4.hand
  script(data-main='/js/index.js' src='/js/lib/require.js')
